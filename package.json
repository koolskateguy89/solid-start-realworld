{
  "name": "solid-start-realworld",
  "scripts": {
    "dev": "solid-start dev",
    "build": "solid-start build && pnpm postbuild",
    "start": "solid-start start",
    "lint-check": "eslint \"**/*.{ts,tsx,js,jsx}\"",
    "lint": "eslint --fix \"**/*.{ts,tsx,js,jsx}\"",
    "push": "prisma db push",
    "postinstall": "prisma generate",
    "postbuild": "cp node_modules/.pnpm/**/**/prisma/*engine*.node .vercel/output/functions/render.func/ && cp prisma/schema.prisma .vercel/output/functions/render.func/ && cp node_modules/.pnpm/**/**/prisma/*engine*.node .vercel/output/functions/api.func/ && cp prisma/schema.prisma .vercel/output/functions/api.func/"
  },
  "type": "module",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.14.5",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-solid": "^0.10.0",
    "prettier": "^2.8.4",
    "prisma": "^4.11.0",
    "solid-start-node": "^0.2.22",
    "solid-start-vercel": "^0.2.22",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.4"
  },
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "@solidjs/meta": "^0.28.2",
    "@solidjs/router": "^0.7.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.0",
    "solid-js": "^1.6.11",
    "solid-start": "^0.2.22",
    "undici": "^5.20.0",
    "zod": "^3.20.6"
  },
  "engines": {
    "node": ">=16"
  },
  "prisma": {
    "seed": "ts-node --transpile-only prisma/seed.ts"
  }
}