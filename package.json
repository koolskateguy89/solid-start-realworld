{
  "name": "solid-start-realworld",
  "scripts": {
    "dev": "solid-start dev",
    "build": "solid-start build && pnpm postbuild",
    "start": "solid-start start",
    "lint-check": "eslint \"**/*.{ts,tsx,js,jsx}\"",
    "lint": "eslint --fix \"**/*.{ts,tsx,js,jsx}\"",
    "push": "prisma db push",
    "postinstall": "prisma generate",
    "postbuild": "cp node_modules/.pnpm/**/**/prisma/*engine*.node .vercel/output/functions/render.func/ && cp prisma/schema.prisma .vercel/output/functions/render.func/ && cp node_modules/.pnpm/**/**/prisma/*engine*.node .vercel/output/functions/api.func/ && cp prisma/schema.prisma .vercel/output/functions/api.func/"
  },
  "type": "module",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.3.2",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-solid": "^0.12.1",
    "prettier": "^2.8.8",
    "prisma": "^4.16.1",
    "solid-start-node": "^0.2.26",
    "solid-start-vercel": "^0.2.26",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "vite": "^4.3.9"
  },
  "dependencies": {
    "@prisma/client": "^4.16.1",
    "@solidjs/meta": "^0.28.5",
    "@solidjs/router": "^0.8.2",
    "@vercel/postgres": "^0.4.0",
    "bcryptjs": "^2.4.3",
    "highlight.js": "^11.8.0",
    "jsonwebtoken": "^9.0.0",
    "solid-highlight": "^0.1.12",
    "solid-js": "^1.7.7",
    "solid-markdown": "^1.2.2",
    "solid-start": "^0.2.26",
    "undici": "^5.22.1",
    "zod": "^3.21.4"
  },
  "engines": {
    "node": ">=16"
  },
  "prisma": {
    "seed": "ts-node --transpile-only prisma/seed.ts"
  }
}